# <file system>	<mount pt>	<type>	<options>	<dump>	<pass>
devpts		/dev/pts	devpts	defaults,gid=5,mode=620,ptmxmode=0666	0	0
tmpfs		/dev/shm	tmpfs	mode=0777	0	0
tmpfs		/tmp		tmpfs	mode=1777	0	0
tmpfs		/run		tmpfs	mode=0755,nosuid,nodev	0	0
tmpfs		/root/.cache		tmpfs	mode=0755,nosuid,nodev	0	0

/dev/mmcblk2p1  /userdata   	auto    defaults,rw 0   1
/dev/mmcblk2  	/userdata   	auto    defaults,rw 0   1
/dev/mmcblk1p3  /userdata   	auto    defaults,rw 0   1
/dev/mmcblk1p2  /userdata   	auto    defaults,rw 0   1
